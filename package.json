{
  "name": "@iamkenos/kyoko",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "prepare": "husky install",
    "release:first": "standard-version --first-release",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "prebuild": "rimraf build && rimraf *.tgz",
    "build": "tsc --build tsconfig.build.json",
    "postbuild": "resolve-tspaths",
    "pack": "npm run build && npm pack",
    "lint": "eslint ./",
    "serve:demo": "http-server ./demo/app -s",
    "test:demo": "start-test serve:demo 8080 cucumber:demo",
    "cucumber:demo": "cucumber-js -c demo/test/cucumber.js"
  },
  "keywords": [],
  "author": "iamkenos <alexis.matunog@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "@cucumber/cucumber": "^10.0.1",
    "@playwright/test": "^1.40.0",
    "@types/fs-extra": "^11.0.4",
    "callsites": "^3.1.0",
    "change-case": "^4.1.2",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@types/glob": "^8.1.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-standard": "^17.1.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "resolve-tspaths": "^0.8.17",
    "start-server-and-test": "^2.0.3"
  }
}
